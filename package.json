{
  "name": "aws-sls-app",
  "version": "0.1.0",
  "bin": {
    "aws-sls-app": "bin/aws-sls-app.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "cdk": "cdk",
    "build": "tsc",
    "test": "jest",
    "build:backend": "GO111MODULE=off go get -v -t -d ./src/backend/persons/... && GOOS=linux GOARCH=amd64 go build -o ./src/backend/persons/persons ./src/backend/persons/**.go",
    "deploy": "npm run build && npm run build:backend && npm run deploy:backend",
    "deploy:backend": "cdk deploy BackendStack"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.72.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "1.72.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.72.0",
    "@aws-cdk/aws-dynamodb": "^1.72.0",
    "@aws-cdk/aws-lambda": "^1.72.0",
    "@aws-cdk/aws-logs": "^1.72.0",
    "@aws-cdk/aws-ssm": "^1.72.0",
    "@aws-cdk/core": "1.72.0",
    "http-method-enum": "^1.0.0",
    "source-map-support": "^0.5.16"
  }
}
